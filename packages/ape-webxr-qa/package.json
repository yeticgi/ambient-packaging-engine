{
    "private": true,
    "name": "@yeti-cgi/ape-webxr-qa",
    "version": "0.0.5",
    "scripts": {
        "build": "rollup -c",
        "build:dev": "rollup -c --configMode dev",
        "build:all": "yarn workspace @yeti-cgi/ape build && yarn workspace @yeti-cgi/ape-reactcomponents build && yarn build",
        "build:all:dev": "yarn workspace @yeti-cgi/ape build && yarn workspace @yeti-cgi/ape-reactcomponents build && yarn build:dev",
        "watch": "rollup -cw",
        "serve": "http-server ./dist -p 5000 -c0"
    },
    "description": "Web XR test application useful for quality assurance testing across many devices.",
    "repository": "git@github.com:yeticgi/ambient-packaging-engine.git",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "http-server": "^0.12.3",
        "rollup": "^2.44.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-delete": "^1.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-postcss": "3.1.8",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@yeti-cgi/ape": "*",
        "@yeti-cgi/ape-reactcomponents": "*",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tslib": "^2.2.0"
    },
    "author": "YETi CGI",
    "files": [
        "dist"
    ]
}
