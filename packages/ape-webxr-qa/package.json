{
    "private": true,
    "name": "@yeticgi/ape-webxr-qa",
    "version": "0.0.2",
    "scripts": {
        "build": "rollup -c",
        "build:dev": "rollup -c --configMode dev",
        "build:all": "yarn workspace @yeticgi/ape build && yarn workspace @yeticgi/ape-reactcomponents build && yarn build",
        "build:all:dev": "yarn workspace @yeticgi/ape build && yarn workspace @yeticgi/ape-reactcomponents build && yarn build:dev",
        "watch": "rollup -cw",
        "serve": "http-server ./dist -p 5000 -c-1"
    },
    "description": "Web XR test application useful for quality assurance testing across many devices.",
    "repository": "git@github.com:yeticgi/ambient-packaging-engine.git",
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "http-server": "^0.12.0",
        "rollup": "^1.27.13",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-copy": "^3.1.0",
        "rollup-plugin-delete": "^1.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-terser": "^5.1.3",
        "rollup-plugin-typescript2": "^0.25.3",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "@types/stats.js": "^0.17.0",
        "@yeticgi/ape": "*",
        "@yeticgi/ape-reactcomponents": "*",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "stats.js": "^0.17.0",
        "tslib": "^1.10.0"
    }
}
